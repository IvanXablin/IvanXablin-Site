{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"warn",
        "quotes":["error", "single"],
        "jsx-quotes":["error", "prefer-double"],
        "prefer-const":"error",
        "semi":"warn",
        "import/order":["error", {
            "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"]
        }]
    }
}